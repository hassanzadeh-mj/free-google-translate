{
  "name": "react-google-translate-mj",
  "version": "1.1.8",
  "description": "A free and simple translation package using the unofficial Google Translate API.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "dev": "rollup -c rollup.config.mjs -w",
    "lint": "eslint src_ts --ext .ts,.tsx",
    "format": "prettier --write \"src_ts/**/*.{ts,tsx}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src_ts",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "translate",
    "google-translate",
    "free",
    "api",
    "language",
    "translation",
    "provider",
    "typescript",
    "react"
  ],
  "author": "Moji <mj.hassanzadeh1995@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hassanzadeh-mj/free-google-translate"
  },
  "bugs": {
    "url": "https://github.com/hassanzadeh-mj/free-google-translate/issues"
  },
  "homepage": "https://github.com/hassanzadeh-mj/free-google-translate#readme"
}
